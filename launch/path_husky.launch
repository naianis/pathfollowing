<?xml version="1.0" encoding="UTF-8"?>

<launch>
  <!-- gazebo args -->
  <arg name="world_name" default="$(find visual_path_husky)worlds/blender/first_blender_try.world"/>
  <arg name="paused" default="false" doc="Starts gazebo in paused mode" />
  <arg name="gui" default="true" doc="Starts gazebo gui" />

  <!--  husky_gazebo args -->
  <arg name="laser_enabled" default="true" doc="Enable husky laser"/>
  <arg name="kinect_enabled" default="true" doc="Enable husky kinect"/>
  <arg name="robot_namespace" default="/"/>

  <!-- include world launcher-->
  <include file="$(find visual_path_husky)launch/path_world.launch">
    <arg name="world_name" value="$(arg world_name)"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="paused" value="$(arg paused)"/>
  </include>
  
  <!-- include husky_gazebo launcher -->
  <include file="$(find husky_gazebo)/launch/spawn_husky.launch">
    <arg name="laser_enabled" value="$(arg laser_enabled)"/>
    <arg name="kinect_enabled" value="$(arg kinect_enabled)"/>
    <arg name="robot_namespace" value="$(arg robot_namespace)"/>
    <arg name="x" value="0.6"/>
    <arg name="y" value="-3.5"/>
    <arg name="z" value="0.02"/>
    <arg name="yaw" value="3.1415"/>
  </include>
</launch>
